  [ INFLECTED [ PST-FLAG +,
                REM-PST-FLAG #rem-pst,
                THIRD-PERSON-FLAG #third-person,
                CASE-PC-FLAG #case-pc,
                VERB-PC62-FLAG #verb-pc62 ],
    DTR pst-rule-dtr &
        [ INFLECTED [ REM-PST-FLAG #rem-pst,
                      THIRD-PERSON-FLAG #third-person,
                      CASE-PC-FLAG #case-pc,
                      VERB-PC62-FLAG #verb-pc62 ] ],
    SYNSEM.LOCAL.CONT.HOOK.INDEX.E.TENSE pst ].

third-person-lex-rule := add-only-no-ccont-rule & infl-lex-rule &
  [ INFLECTED [ THIRD-PERSON-FLAG +,
                PST-FLAG #pst,
                REM-PST-FLAG #rem-pst,
                CASE-PC-FLAG #case-pc,
                VERB-PC62-FLAG #verb-pc62 ],
    DTR third-person-rule-dtr &
        [ INFLECTED [ PST-FLAG #pst,
                      REM-PST-FLAG #rem-pst,
                      CASE-PC-FLAG #case-pc,
                      VERB-PC62-FLAG #verb-pc62 ] ] ].

